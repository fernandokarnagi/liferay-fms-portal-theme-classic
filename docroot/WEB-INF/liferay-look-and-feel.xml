<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.4+</version>
	</compatibility>
	<theme id="liferay-fms-portal-classic" name="Liferay Fms Portal Classic">
		<settings>
			<setting configurable="true" key="header" type="checkbox"
				value="true" />
			<setting configurable="true" key="footer" type="checkbox"
				value="true" />
			<setting configurable="true" key="docbar" type="checkbox"
				value="true" />
			<setting configurable="true" key="breadcrumb" type="checkbox"
				value="true" />
			<setting configurable="true" key="navigation" type="checkbox"
				value="true" />
			<setting configurable="true" key="poweredby" type="textarea"
				value="CTC Global" />
			<setting configurable="true" key="portlet-setup-show-borders-default"
				type="checkbox" value="false" />
			<setting configurable="true" key="bullet-style" options="arrows,dots,classic,modern"
				value="dots" />
			<setting configurable="true" key="show-site-name-default"
				type="checkbox" value="true" />
			<setting configurable="true" key="show-site-name-supported"
				type="checkbox" value="true" />
		</settings>
	</theme>
</look-and-feel>